<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.xpup.hafmis.signjoint.entity">

	<class name="Sign" table="RQ001">
    	<id name="id" type="java.lang.Integer" column="ID">
            <generator class="native">         
                <param name="sequence">SEQ_RQ001</param>
            </generator>
		</id>
		<property name="sign" type="java.lang.Integer" column="SIGN" />        
	    <property name="name" type="java.lang.String" column="NAME" />        
	    <property name="card_num" type="java.lang.String" column="CARD_NUM" />        
	    <property name="empid" type="java.lang.String" column="EMPID" />        
	    <property name="bank_card" type="java.lang.String" column="BANK_CARD" />        
	</class>
</hibernate-mapping>