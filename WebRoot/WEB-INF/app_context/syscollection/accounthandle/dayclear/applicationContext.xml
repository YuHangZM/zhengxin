<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<!-- Application context definition for collection's business layer. -->
<beans>
	<!-- ========================= BUSINESS OBJECT DEFINITIONS ======================== -->
	<bean id="dayclearBS" parent="baseTransactionProxy">
		<property name="target">
			<bean
				class="org.xpup.hafmis.syscollection.accounthandle.dayclear.business.DayclearBS">
				<property name="securityDAO">
					<ref bean="securityDAO" />
				</property>	
				<property name="loanBankParaDAO">
					<ref bean="loanBankParaDAO" />
				</property>				
			</bean>
		</property>
	</bean>
	
</beans>

